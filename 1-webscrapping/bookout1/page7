booking.env.b_context_details = [];
booking.env.b_context_details.push( { "b_type" : "city", "b_code" : "20088325" } );
booking.env.b_multiple_destinations_found = '';
booking.env.b_acc_type_ga = '';
booking.env.ga_clean_www_js = 1;
if (typeof _gaq == 'object' || (window.ga && typeof window.ga === 'function')) {
